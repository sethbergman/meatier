    "body-parser": "1.18.1",
    "compression": "1.7.0",
    "cors": "2.8.4",
    "deep-equal": "1.0.1",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.0.1",
    "es6-promisify": "5.0.0",
    "express": "4.15.4",
    "express-jwt": "5.3.0",
    "googleapis": "21.3.0",
    "graphiql": "0.11.3",
